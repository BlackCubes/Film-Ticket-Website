{
  "name": "kinetotickets",
  "version": "1.0.0",
  "description": "A Ticketing Website that users can geographically locate, create, and buy rare tickets from classical movies to television shows, and to limited time and special venue shows with the casts and crews as special guests.",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:prod": "SET NODE_ENV=production&&nodemon server.js",
    "debug": "ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "watch:sass": "node-sass public/sass/main.scss public/css/style.css -w",
    "compile:sass": "node-sass public/sass/main.scss public/css/style.comp.css",
    "concat:css": "concat -o public/css/style.concat.css public/css/all.css public/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' public/css/style.concat.css -o public/css/style.prefix.css",
    "compress:css": "node-sass public/css/style.prefix.css public/css/style.min.css --output-style compressed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BlackCubes/Film-Ticket-Website.git"
  },
  "author": "Elias Gutierrez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BlackCubes/Film-Ticket-Website/issues"
  },
  "homepage": "https://github.com/BlackCubes/Film-Ticket-Website#readme",
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.22.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "datauri": "^3.0.0",
    "dotenv": "^7.0.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^1.3.2",
    "express-rate-limit": "^3.5.3",
    "express-useragent": "^1.0.15",
    "gsap": "^3.3.1",
    "helmet": "^3.22.0",
    "hpp": "^0.2.3",
    "html-to-text": "^5.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.6",
    "pug": "^2.0.4",
    "rellax": "^1.12.1",
    "sanitize-html": "^1.27.4",
    "sharp": "^0.24.1",
    "slugify": "^1.4.0",
    "streamifier": "^0.1.1",
    "stripe": "^7.15.0",
    "swiper": "^6.1.1",
    "validator": "^13.0.0",
    "validatorjs": "^3.19.2",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.0.0",
    "concat": "^1.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "postcss-cli": "^7.1.2",
    "prettier": "^1.19.1"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
